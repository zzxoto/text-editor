{
  "env": {
      "node": true,
      "commonjs": true,
      "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
      "indent": [
          1,
          2
      ],
      "quotes": [
          2,
          "single"
      ],
      "semi": [
          2,
          "always"
      ],
      "no-console": [
          0
      ]
  }
}